#
# this is ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

source ~/.shell_aliases

PS1='[\u@\h \W]\$ '
. "$HOME/.cargo/env"

export PATH=$PATH:/home/aki/.spicetify
export PATH=~/bin/prepend:$PATH:~/bin
export PATH=$PATH:~/dev/vcpkg
export VCPKG_TOOLCHAIN_FILE=/home/aki/dev/vcpkg/scripts/buildsystems/vcpkg.cmake
export HYPRSHOT_DIR=$XDG_PICTURES_DIR/hyprshot
eval "$(starship init bash)"
. "/home/aki/.deno/env"
